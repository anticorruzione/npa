        <dc:Bounds height="80" width="180" x="200" y="80" />
      </dmndi:DMNShape>
      <dmndi:DMNEdge id="DMNEdge_0d85lf4" dmnElementRef="InformationRequirement_A2_30_02">
        <di:waypoint x="360" y="247" />
        <di:waypoint x="320" y="180" />
        <di:waypoint x="320" y="160" />
      </dmndi:DMNEdge>
