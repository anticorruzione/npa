<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:modeler="http://camunda.org/schema/modeler/1.0" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Scheda_RSU1" name="Regole" namespace="http://camunda.org/schema/1.0/dmn" exporter="dmn-js (https://demo.bpmn.io/dmn)" exporterVersion="17.2.0" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0" camunda:diagramRelationId="ad8e9e2b-ffef-4291-b177-ce7ee3ca7fa8">
  <decision id="RSU1" name="RSU1">
    <informationRequirement id="InformationRequirement_00o5h7n">
      <requiredInput href="#InputData_1vsdrei" />
    </informationRequirement>
    <informationRequirement id="InformationRequirement_0558eb9">
      <requiredInput href="#InputData_19wlu85" />
    </informationRequirement>
    <decisionTable id="DecisionTable_RSU1" hitPolicy="COLLECT">
      <input id="appalto_RSU1" biodi:width="394">
        <inputExpression id="LiteralExpression_appalto_RSU1" typeRef="string">
          <text>subappalto</text>
        </inputExpression>
      </input>
      <input id="InputClause_0ge2vg3">
        <inputExpression id="LiteralExpression_030x2vz" typeRef="string">
          <text>idSubappalto</text>
        </inputExpression>
      </input>
      <output id="Errore_AD1_10" name="Errore" typeRef="string" biodi:width="477" />
      <rule id="DecisionRule_0m0ht0d">
        <description>REG0: Dati non presenti</description>
        <inputEntry id="UnaryTests_0qvvbpl">
          <text>subappalto = null or subappalto = ""</text>
        </inputEntry>
        <inputEntry id="UnaryTests_07r80pl">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0nyzfph">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG0\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0o1lpaj">
        <description>REG138: I campi di tipo testo non possono essere vuoti</description>
        <inputEntry id="UnaryTests_0li798j">
          <text>if (subappalto.operatoreEconomicoSubappaltatore.idOE="")</text>
        </inputEntry>
        <inputEntry id="UnaryTests_04dmj24">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_13ike9x">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG138\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_146nxje">
        <description>REG139: Definire almeno un conferente subappalto</description>
        <inputEntry id="UnaryTests_0eervl7">
          <text>if ((is defined(subappalto.conferenteSubappalto.idAggiudicatario)=false and is defined(subappalto.conferenteSubappalto.idSubappaltanteACascata)=false) or (subappalto.conferenteSubappalto.idAggiudicatario="" and subappalto.conferenteSubappalto.idSubappaltanteACascata=""))</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0iy35t5">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0a9hrvo">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG139\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1ec3b6s">
        <description>REG140: Definire un solo conferente subappalto</description>
        <inputEntry id="UnaryTests_0g025qn">
          <text>if ((is defined(subappalto.conferenteSubappalto.idAggiudicatario) and is defined(subappalto.conferenteSubappalto.idSubappaltanteACascata)) and (subappalto.conferenteSubappalto.idAggiudicatario!="" and subappalto.conferenteSubappalto.idSubappaltanteACascata!=""))</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0lzm2lg">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0bdxjfd">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG140\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1ys5s32">
        <description>REG141: In caso di subappalto a cascata deve essere indicato l'idSubappalto</description>
        <inputEntry id="UnaryTests_1ye9vw4">
          <text>if is defined(subappalto.conferenteSubappalto.idSubappaltanteACascata) and subappalto.conferenteSubappalto.idSubappaltanteACascata!=""</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1x4zs9m">
          <text>is defined(idSubappalto) = false or idSubappalto=""</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_17zdmx5">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG141\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_188b0qk">
        <description>REG142: Indicare almeno un soggetto</description>
        <inputEntry id="UnaryTests_0ae0sya">
          <text>if (count(subappalto.operatoreEconomicoSubappaltatore.elencoSoggetti) = 0)</text>
        </inputEntry>
        <inputEntry id="UnaryTests_16wxygi">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1a51bpd">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG142\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1b9wz4k">
        <description>REG2: indicare il codice fiscale del soggetto.</description>
        <inputEntry id="UnaryTests_0fg9rak">
          <text>some soggetto in subappalto.operatoreEconomicoSubappaltatore.elencoSoggetti
satisfies (soggetto.codiceFiscale = null or soggetto.codiceFiscale="")</text>
        </inputEntry>
        <inputEntry id="UnaryTests_10ef4ln">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0ak9i0h">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG2\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_126fqqi">
        <description>REG129: indicare la denominazione del soggetto.</description>
        <inputEntry id="UnaryTests_0derb7g">
          <text>some soggetto in subappalto.operatoreEconomicoSubappaltatore.elencoSoggetti
satisfies (soggetto.denominazione = null or soggetto.denominazione="")</text>
        </inputEntry>
        <inputEntry id="UnaryTests_1hqfmbh">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_1fgef34">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG129\"}"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
