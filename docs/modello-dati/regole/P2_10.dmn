          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_P2_10_0009">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG35\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_P2_10_0010">
