<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="https://www.omg.org/spec/DMN/20191111/MODEL/" xmlns:dmndi="https://www.omg.org/spec/DMN/20191111/DMNDI/" xmlns:dc="http://www.omg.org/spec/DMN/20180521/DC/" xmlns:di="http://www.omg.org/spec/DMN/20180521/DI/" xmlns:biodi="http://bpmn.io/schema/dmn/biodi/2.0" xmlns:modeler="http://camunda.org/schema/modeler/1.0" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" id="Scheda_A3_2" name="Regole" namespace="http://camunda.org/schema/1.0/dmn" exporter="dmn-js (https://demo.bpmn.io/dmn)" exporterVersion="14.1.1" modeler:executionPlatform="Camunda Cloud" modeler:executionPlatformVersion="8.2.0" camunda:diagramRelationId="ad8e9e2b-ffef-4291-b177-ce7ee3ca7fa8">
  <inputData id="InputData_aggiudicazioni" name="aggiudicazioni" />
  <inputData id="InputData_appalto" name="appalto" />
  <decision id="A3_2" name="A3_2">
    <informationRequirement id="InformationRequirement_A3_2_02">
      <requiredInput href="#InputData_aggiudicazioni" />
    </informationRequirement>
    <informationRequirement id="InformationRequirement_A3_2_01">
      <requiredInput href="#InputData_appalto" />
    </informationRequirement>
    <decisionTable id="DecisionTable_A3_2" hitPolicy="COLLECT">
      <input id="appalto_A3_2">
        <inputExpression id="LiteralExpression_appalto_A3_2" typeRef="string">
          <text>appalto</text>
        </inputExpression>
      </input>
      <input id="lotti_P1_10" biodi:width="426">
        <inputExpression id="LiteralExpression_aggiudicazioni_A3_2" typeRef="string">
          <text>aggiudicazioni</text>
        </inputExpression>
      </input>
      <output id="Errore_AD1_10" name="Errore" typeRef="string" biodi:width="477" />
      <rule id="DecisionRule_1ub3t5l">
        <inputEntry id="UnaryTests_17did8m">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_0o5k2bd">
          <text>aggiudicazioni = null</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_05255um">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"ERR41\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1375p7w">
        <description>REG47: Il numero di offerte presentate non deve essere superiore al numero di offerte ammesse</description>
        <inputEntry id="UnaryTests_0an2yyy">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_1extrxn">
          <text>some aggiudicazione in aggiudicazioni
satisfies (count(aggiudicazione.offertePresentate) &gt; aggiudicazione.numeroOfferteAmmesse)</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_01kmogs">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG47\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0mzon1b">
        <description>REG50: Il valore del campo “offertaEconomica” non è coerente</description>
        <inputEntry id="UnaryTests_0ogv8jb">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_1x5logn">
          <text>some aggiudicazione in aggiudicazioni
satisfies (some offerta in aggiudicazione.offertePresentate 
satisfies offerta.offertaEconomica &lt; 0 OR offerta.offertaEconomica &gt; 100) </text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0aiqqkm">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG50\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0a8jibc">
        <description>REG51: Richiesto almeno un valore tra offertaEconomica e offertaInAumento</description>
        <inputEntry id="UnaryTests_0jr5sv8">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_058nzm7">
          <text>some aggiudicazione in aggiudicazioni
satisfies (count(aggiudicazione.offertePresentate) = 1 AND some offerta in aggiudicazione.offertePresentate 
satisfies offerta.offertaInAumento = 0 AND offerta.offertaEconomica = 0)</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0rivwtj">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG51\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0uku76x">
        <description>REG52: Non è stato inserito l’importo dell’aggiudicatario</description>
        <inputEntry id="UnaryTests_0zn2el7">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_0h500t2">
          <text>some aggiudicazione in aggiudicazioni
satisfies count(aggiudicazione.offertePresentate)&gt;1 and every offerta in aggiudicazione.offertePresentate satisfies (offerta.importo = null or offerta.importo &lt;= 0)</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0ttgkpq">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG52\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_1vm8pxi">
        <description>REG69: Quadro economico concessioni - Importo Lavori: se l'oggetto principale del contratto è "Lavori" deve essere indicato un importo maggiore di 0.</description>
        <inputEntry id="UnaryTests_0681b0h">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_1cmyair">
          <text>some aggiudicazione in aggiudicazioni
satisfies aggiudicazione.oggettoPrincipaleContratto.codice = "L" and aggiudicazione.quadroEconomicoConcessioni.impLavori = null or number(aggiudicazione.quadroEconomicoConcessioni.impLavori) = 0</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_164k4in">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG69\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_174p22m">
        <description>REG70: Quadro economico concessioni - Importo Servizi: se l'oggetto principale del contratto è "Servizi" deve essere indicato un importo maggiore di 0.</description>
        <inputEntry id="UnaryTests_0xo7ia7">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_0x3o10l">
          <text>some aggiudicazione in aggiudicazioni
satisfies aggiudicazione.oggettoPrincipaleContratto.codice = "S" and aggiudicazione.quadroEconomicoConcessioni.impServizi = null or number(aggiudicazione.quadroEconomicoConcessioni.impServizi) = 0</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_0iefz8i">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG70\"}"</text>
        </outputEntry>
      </rule>
      <rule id="DecisionRule_0b6gkmh">
        <description>REG71: Quadro economico concessioni - Importo Forniture: se l'oggetto principale del contratto è "Forniture" deve essere indicato un importo maggiore di 0.</description>
        <inputEntry id="UnaryTests_1exttas">
          <text></text>
        </inputEntry>
        <inputEntry id="UnaryTests_0ma8zxj">
          <text>some aggiudicazione in aggiudicazioni
satisfies aggiudicazione.oggettoPrincipaleContratto.codice = "F" and aggiudicazione.quadroEconomicoConcessioni.impForniture = null or number(aggiudicazione.quadroEconomicoConcessioni.impForniture) = 0</text>
        </inputEntry>
        <outputEntry id="LiteralExpression_02s7yd4">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG71\"}"</text>
        </outputEntry>
      </rule>
    </decisionTable>
  </decision>
