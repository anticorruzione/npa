 date and time(substring before(lotto.datiBaseTerminiInvio.scadenzaPresentazioneInvito, "Z")) &lt;= date and time(string(today()))</text>
        </inputEntry>
        <inputEntry id="UnaryTests_0ibafgt">
          <text></text>
        </inputEntry>
        <outputEntry id="LiteralExpression_01bit43">
          <text>"{\"idTipologica\":\"errori\",\"codice\":\"REG106\"}"</text>
        </outputEntry>
      </rule>	  
